# 服务器配置
SERVER_PORT=3000
SERVER_HOST=0.0.0.0

# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=roc
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false
DB_MAX_CONNECTIONS=10

# Redis 配置
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# LLM 配置
LLM_PROVIDER=openai
LLM_API_KEY=your-api-key-here
LLM_MODEL=gpt-4
LLM_MAX_TOKENS=4096
LLM_TEMPERATURE=0.7

# 集群配置
CLUSTER_TYPE=slurm
SLURM_PARTITION=gpu
SLURM_ACCOUNT=
SLURM_QOS=

# Kubernetes 配置 (如果使用 K8s)
K8S_NAMESPACE=default
KUBECONFIG=

# SSH 配置 (如果使用 SSH)
SSH_HOST=
SSH_PORT=22
SSH_USERNAME=
SSH_PRIVATE_KEY_PATH=

# 存储配置
STORAGE_TYPE=local
STORAGE_BASE_PATH=./artifacts

# S3/MinIO 配置 (如果使用对象存储)
S3_BUCKET=roc-artifacts
S3_REGION=us-east-1
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=
S3_ENDPOINT=

# 追踪集成
MLFLOW_ENABLED=false
MLFLOW_TRACKING_URI=http://localhost:5000

WANDB_ENABLED=false
WANDB_API_KEY=
WANDB_PROJECT=
WANDB_ENTITY=

TENSORBOARD_ENABLED=true
TENSORBOARD_LOG_DIR=./tensorboard_logs

# memU 配置
MEMU_ENABLED=false
MEMU_HOST=localhost
MEMU_PORT=50051
MEMU_USE_TLS=false

# 日志级别
LOG_LEVEL=info
NODE_ENV=development
